{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "env": ["NEXTAUTH_URL", "AUTH_GOOGLE_ID", "AUTH_GOOGLE_SECRET"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "dependsOn": ["db:build", "db:generate", "^dev"],
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:format": {
      "cache": false
    },
    "db:reset": {
      "cache": false
    },
    "db:migrate": {
      "cache": false,
      "persistent": true
    },
    "db:deploy": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:build": {
      "cache": false,
      "outputs": ["dist/**"]
    },
    "db:seed": {
      "cache": false
    },
    "deploy": {
      "cache": false
    },
    "services:build": {
      "cache": false,
      "outputs": ["dist/**"]
    },
    "services:watch": {
      "cache": false,
      "persistent": true
    }
  }
}
